{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <h1>{{ heading_title }}</h1>
      <ul class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ul>
    </div>
  </div>
  <div class="container-fluid">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><i class="fa fa-list"></i> {{ text_list }}</h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-4">
            <div class="panel panel-info">
              <div class="panel-heading">
                <h3 class="panel-title">{{ heading_trial_balance }}</h3>
              </div>
              <div class="panel-body">
                <p>{{ text_trial_balance_info }}</p>
                <a href="{{ trial_balance }}" class="btn btn-primary btn-block"><i class="fa fa-list-alt"></i> {{ button_view }}</a>
              </div>
            </div>
          </div>
          <div class="col-md-4">
            <div class="panel panel-success">
              <div class="panel-heading">
                <h3 class="panel-title">{{ heading_income_statement }}</h3>
              </div>
              <div class="panel-body">
                <p>{{ text_income_statement_info }}</p>
                <a href="{{ income_statement }}" class="btn btn-primary btn-block"><i class="fa fa-bar-chart"></i> {{ button_view }}</a>
              </div>
            </div>
          </div>
          <div class="col-md-4">
            <div class="panel panel-warning">
              <div class="panel-heading">
                <h3 class="panel-title">{{ heading_balance_sheet }}</h3>
              </div>
              <div class="panel-body">
                <p>{{ text_balance_sheet_info }}</p>
                <a href="{{ balance_sheet }}" class="btn btn-primary btn-block"><i class="fa fa-balance-scale"></i> {{ button_view }}</a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
{{ footer }}
